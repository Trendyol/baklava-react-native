{
  "name": "baklava-react-native",
  "version": "0.0.1",
  "private": true,
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/src/index.d.ts",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestart": "sbn-get-stories",
    "sb-watcher": "sbn-watcher",
    "pod": "cd ios; pod install; cd ..;",
    "prepare": "bob build",
    "svgr": "svgr ./src/svg-icons -d ./src/icons --native --svg-props 'className=' --typescript --svgo-config svgo.config.js --ignore-existing --no-index"
  },
  "dependencies": {
    "@svgr/cli": "^6.2.1",
    "react": "17.0.2",
    "react-native": "0.67.4",
    "react-native-svg": "^12.3.0",
    "styled-components": "5.3.0",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-async-storage/async-storage": "1.17.7",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/slider": "4.1.8",
    "@storybook/addon-actions": "6.3.10",
    "@storybook/addon-controls": "6.3.10",
    "@storybook/addon-ondevice-actions": "6.0.1-alpha.3",
    "@storybook/addon-ondevice-backgrounds": "6.0.1-alpha.3",
    "@storybook/addon-ondevice-controls": "6.0.1-alpha.3",
    "@storybook/addon-ondevice-notes": "6.0.1-alpha.3",
    "@storybook/react-native": "6.0.1-alpha.3",
    "@testing-library/jest-native": "4.0.5",
    "@testing-library/react-native": "10.1.1",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components-react-native": "5.1.3",
    "@types/styled-system": "5.1.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-dom": "^17.0.2",
    "react-native-builder-bob": "^0.18.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ],
    "files": [
      "lib/",
      "src/",
      "components/"
    ]
  }
}
